// Template parameters
var from_db string
var from_rp string
var measurement string
var qy = 'select mean(*) from "' + from_db + '"."' + from_rp + '"."' + measurement + '"'
var one_year = batch
  |query(qy)
    .groupByMeasurement()
    .groupBy(*)
    .period(5m)
    .every(5m)
    .align()
one_year
  |influxDBOut()
    .database(from_db)
    .retentionPolicy('5m_mean_one_year')
    .measurement(measurement)
